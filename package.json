{
  "name": "salesforcedx",
  "version": "51.0.3",
  "description": "The Salesforce DX plugin aggregates all force topic commands into a single plugin that is bundled with the Salesforce CLI.",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "plugins": [
      "@salesforce/plugin-apex",
      "@salesforce/plugin-custom-metadata",
      "@salesforce/plugin-limits",
      "@salesforce/plugin-schema",
      "@salesforce/plugin-templates",
      "@salesforce/plugin-user",
      "@salesforce/sfdx-plugin-lwc-test",
      "salesforce-alm"
    ]
  },
  "pinnedDependencies": [
    "@salesforce/plugin-custom-metadata",
    "@salesforce/plugin-limits",
    "@salesforce/plugin-schema",
    "@salesforce/plugin-templates",
    "@salesforce/plugin-user",
    "@salesforce/sfdx-plugin-lwc-test",
    "salesforce-alm"
  ],
  "dependencies": {
    "@salesforce/plugin-apex": "0.1.4",
    "@salesforce/plugin-custom-metadata": "1.0.11",
    "@salesforce/plugin-limits": "1.0.4",
    "@salesforce/plugin-schema": "1.0.4",
    "@salesforce/plugin-templates": "51.2.0",
    "@salesforce/plugin-user": "1.1.2",
    "@salesforce/sfdx-plugin-lwc-test": "0.1.7",
    "salesforce-alm": "51.0.2",
    "salesforce-lightning-cli": "3.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@salesforce/dev-config": "^2.0.0",
    "@salesforce/dev-scripts": "0.7.0",
    "@salesforce/prettier-config": "^0.0.1",
    "@salesforce/ts-sinon": "1.3.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "chai-as-promised": "5.3.0",
    "chai-fs": "1.0.0",
    "chai-string": "1.5.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "sinon": "^9.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "homepage": "https://github.com/salesforcecli/salesforcedx",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "repository": "salesforcecli/salesforcedx",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "eslint 'src/**/*.ts'",
    "prepack": "sf-build",
    "test": "echo 'Salesforcedx is an aggregator plugin that does not contain any tests'",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sf-husky-pre-commit",
      "commit-msg": "sf-husky-commit-msg",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
