{
    "name": "salesforcedx",
    "version": "50.14.1",
    "description": "The Salesforce DX plugin aggregates all force topic commands into a single plugin that is bundled with the Salesforce CLI.",
    "author": "Salesforce",
    "bugs": "https://github.com/forcedotcom/cli/issues",
    "files": [
        "oclif.manifest.json",
        "lib/**/*.js",
        "/messages",
        "/oclif.manifest.json"
    ],
    "oclif": {
        "commands": "./lib/commands",
        "plugins": [
            "salesforce-alm",
            "@salesforce/plugin-templates",
            "@salesforce/sfdx-plugin-lwc-test",
            "@salesforce/plugin-custom-metadata",
            "@salesforce/plugin-apex",
            "@salesforce/plugin-schema",
            "@salesforce/plugin-user"
        ]
    },
    "pinnedDependencies": [
        "@salesforce/plugin-custom-metadata",
        "@salesforce/plugin-schema",
        "@salesforce/plugin-user",
        "@salesforce/plugin-templates",
        "@salesforce/sfdx-plugin-lwc-test",
        "salesforce-alm"
    ],
    "dependencies": {
        "@salesforce/plugin-apex": "0.1.4",
        "@salesforce/plugin-custom-metadata": "1.0.11",
        "@salesforce/plugin-schema": "1.0.3",
        "@salesforce/plugin-user": "1.0.7",
        "@salesforce/plugin-templates": "50.4.0",
        "@salesforce/sfdx-plugin-lwc-test": "0.1.7",
        "salesforce-alm": "50.14.0",
        "salesforce-lightning-cli": "3.0.0",
        "@oclif/config": "^1.17.0",
        "@salesforce/command": "^3.0.5",
        "@salesforce/core": "^2.15.2",
        "tslib": "^1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@salesforce/dev-config": "^2.0.0",
        "@salesforce/dev-scripts": "0.6.2",
        "@salesforce/prettier-config": "^0.0.1",
        "@salesforce/ts-sinon": "1.1.3",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "chai": "^4.2.0",
        "chai-as-promised": "5.3.0",
        "chai-fs": "1.0.0",
        "chai-string": "1.2.0",
        "cz-conventional-changelog": "^3.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "5.0.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-salesforce": "^0.1.0",
        "eslint-config-salesforce-license": "^0.1.0",
        "eslint-config-salesforce-typescript": "^0.1.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsdoc": "^27.0.3",
        "eslint-plugin-prettier": "^3.1.3",
        "husky": "^4.2.5",
        "mocha": "^7.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "sinon": "^9.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.3"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "homepage": "https://github.com/salesforcecli/salesforcedx",
    "keywords": [
        "force",
        "salesforce",
        "sfdx",
        "salesforcedx",
        "sfdx-plugin"
    ],
    "license": "BSD-3-Clause",
    "repository": "salesforcecli/salesforcedx",
    "scripts": {
        "build": "sf-build",
        "clean": "sf-clean",
        "clean-all": "sf-clean all",
        "compile": "sf-compile",
        "docs": "sf-docs",
        "format": "sf-format",
        "lint": "eslint 'src/**/*.ts'",
        "prepack": "sf-build",
        "test": "echo 'Salesforcedx is an aggregator plugin that does not contain any tests'",
        "version": "oclif-dev readme && git add README.md"
    },
    "husky": {
        "hooks": {
            "pre-commit": "sf-husky-pre-commit",
            "commit-msg": "sf-husky-commit-msg",
            "pre-push": "sf-husky-pre-push"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
